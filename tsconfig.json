{
  "compilerOptions": {
    "lib": ["es5", "es6"],
    "types": ["node", "express"],
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./build",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": false,
    "skipLibCheck": true,
    "downlevelIteration": true
  }
}
